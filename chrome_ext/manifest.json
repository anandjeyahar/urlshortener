{
  "manifest_version": 2,

  "name": "UrlShortener",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  "permissions": [
    "http://aang.in/",
  ],
  "content_scripts": [
      "matches": ["*"],
      "js":["urlshorten.js"],
      "run_at":"document_end"

  ]
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
